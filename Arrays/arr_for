public class Arr {
    public static void main(String[] args) {
        int tab[] = new int[7];
        tab[0] = 6;
        tab[1] = 7;
        tab[2] = 43;
        tab[3] = 51;
        tab[4] = 2;
        for (int i=0; i<tab.length; i++){
            System.out.println("Element tablicy = " + tab[i]);
        }
        for (int x : tab){
            System.out.println(x);
        }
    }
}
